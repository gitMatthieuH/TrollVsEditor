<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:style>
		.important {
			font-weight: bold;
		}
	</ui:style>
	<g:HTMLPanel>
		<g:DockPanel width="448px" height="35px">
			<g:Dock direction="CENTER">
				<g:HorizontalPanel>
					<g:Button text="Edition de Troll" ui:field="edittroll"/>
					<g:Button text="Liste des Trolls" ui:field="trolllist"/>
					<g:Button text="Tableau détaillé des Trolls" ui:field="trolltable"/>
				</g:HorizontalPanel>
			</g:Dock>
		</g:DockPanel>
	</g:HTMLPanel>
</ui:UiBinder> 